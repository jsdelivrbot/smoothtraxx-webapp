.smx-home-featured.smx-home-featured--hilite(style="background-image: url(http://geo.static.traxsource.com/files/images/b718363_large.jpg);")
    .smx-home-featured__info
        .smx-home-featured__info__title Now on radio
        span
        .smx-home-featured__info__subtitle Keep Pushing - Full Intention
        span
        .smx-home-featured__info__btn(ng-click="vm.showPlayer()")
            span tune in
            //i.material-icons play_circle_filled
.smx-home-new
